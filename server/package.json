{
  "name": "node-express",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "client-install": "cd ../client && npm install",
    "start": "node ./bin/www",
    "server": "nodemon ./bin/wwww",
    "client": "npm start --prefix ../client",
    "dev": "concurrently \"npm:server\" \"npm:client\""
  },
  "engines": {
    "node": "10.15.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "mysql2": "^1.6.5",
    "nodemailer": "^5.1.1",
    "pg": "^7.11.0",
    "sequelize": "^5.8.5"
  },
  "devDependencies": {
    "nodemon": "^1.18.11",
    "rimraf": "^2.6.3"
  }
}
